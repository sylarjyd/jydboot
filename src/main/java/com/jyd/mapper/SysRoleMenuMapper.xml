<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jyd.mapper.SysRoleMenuMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="sysRoleMenuResultMap" type="com.jyd.entity.SysRoleMenu">
        <id column="id" property="id"/>
        <result column="role_id" property="roleId"/>
        <result column="menu_id" property="menuId"/>
    </resultMap>


    <sql id="Base_Column_List">
        
        role_id, menu_id
    </sql>

    <select id="selectSysRoleMenuPage" resultMap="sysRoleMenuResultMap">
        select * from t_sys_role_menu where deleted = 0
    </select>

</mapper>
