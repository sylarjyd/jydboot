<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jyd.mapper.FileMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="fileResultMap" type="com.jyd.entity.OssFile">
        <id column="id" property="id"/>
        <result column="file_name" property="fileName"/>
        <result column="original_name" property="originalName"/>
        <result column="file_type" property="fileType"/>
        <result column="file_size" property="fileSize"/>
        <result column="auth" property="auth"/>
        <result column="create_user" property="createUser"/>
        <result column="create_time" property="createTime"/>
        <result column="update_user" property="updateUser"/>
        <result column="update_time" property="updateTime"/>
        <result column="status" property="status"/>
        <result column="deleted" property="deleted"/>
        <result column="domain" property="domain"/>
        <result column="bucket" property="bucket"/>
        <result column="url" property="url"/>
    </resultMap>


    <sql id="Base_Column_List">

        file_name, original_name, file_type, file_size, auth, create_user, create_time, update_user, update_time, status, deleted, domain, bucket,url
    </sql>

    <select id="selectFilePage" resultMap="fileResultMap">
        select * from t_file where deleted = 0
    </select>

</mapper>
