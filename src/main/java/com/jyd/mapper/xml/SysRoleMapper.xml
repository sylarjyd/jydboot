<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jyd.mapper.SysRoleMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="sysRoleResultMap" type="com.jyd.entity.SysRole">
        <id column="id" property="id"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
        <result column="create_user" property="createUser"/>
        <result column="update_user" property="updateUser"/>
        <result column="status" property="status"/>
        <result column="deleted" property="deleted"/>
        <result column="role_name" property="roleName"/>
        <result column="mark" property="mark"/>
    </resultMap>


    <sql id="Base_Column_List">
        
        create_time, update_time, create_user, update_user, status, deleted, role_name, mark
    </sql>

    <select id="selectSysRolePage" resultMap="sysRoleResultMap">
        select * from t_sys_role where deleted = 0
    </select>

</mapper>
