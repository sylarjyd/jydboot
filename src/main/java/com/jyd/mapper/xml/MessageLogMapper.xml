<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jyd.mapper.MessageLogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="messageLogResultMap" type="com.jyd.entity.MessageLog">
        <id column="id" property="id"/>
        <result column="create_time" property="createTime"/>
        <result column="create_user" property="createUser"/>
        <result column="type" property="type"/>
        <result column="status" property="status"/>
        <result column="title" property="title"/>
        <result column="content" property="content"/>
        <result column="business_id" property="businessId"/>
        <result column="user_id" property="userId"/>
    </resultMap>


    <sql id="Base_Column_List">
        
        create_time, create_user, type, status, title, content, business_id, user_id
    </sql>

    <select id="selectMessageLogPage" resultMap="messageLogResultMap">
        select * from t_message_log where deleted = 0
    </select>

</mapper>
